{
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
    "env": {
        "browser": true,
        "node": true,
        "jquery": true
    },
    "rules": {
        "quotes": 0,
        "no-trailing-spaces": 2,
        "eol-last": 2,
        "no-unused-vars": 0,
        "no-underscore-dangle": 0,
        "no-alert": 0,
        "no-lone-blocks": 2
    },
    "globals": {
        "jQuery": true,
        "$": true
    }
}